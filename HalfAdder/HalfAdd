#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/aarch64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/aarch64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/aarch64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/aarch64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/aarch64-linux-gnu/ivl/va_math.vpi";
S_0x558d8ee320 .scope module, "HalfAddTest_v" "HalfAddTest_v" 2 26;
 .timescale -9 -12;
v0x558d90b1a0_0 .var "A", 0 0;
v0x558d90b290_0 .var "B", 0 0;
v0x558d90b360_0 .net "Cout", 0 0, L_0x558d90b860;  1 drivers
v0x558d90b460_0 .net "Sum", 0 0, L_0x558d90b950;  1 drivers
v0x558d90b530_0 .var "passed", 7 0;
S_0x558d8ee4b0 .scope task, "allPassed" "allPassed" 2 45, 2 45 0, S_0x558d8ee320;
 .timescale -9 -12;
v0x558d8f1b60_0 .var "numTests", 7 0;
v0x558d90a330_0 .var "passed", 7 0;
TD_HalfAddTest_v.allPassed ;
    %load/vec4 v0x558d90a330_0;
    %load/vec4 v0x558d8f1b60_0;
    %cmp/e;
    %jmp/0xz  T_0.0, 4;
    %vpi_call 2 49 "$display", "All tests passed" {0 0 0};
    %jmp T_0.1;
T_0.0 ;
    %vpi_call 2 50 "$display", "Some tests failed" {0 0 0};
T_0.1 ;
    %end;
S_0x558d90a410 .scope module, "dut" "HalfAdd" 2 63, 3 1 0, S_0x558d8ee320;
 .timescale 0 0;
    .port_info 0 /OUTPUT 1 "Cout";
    .port_info 1 /OUTPUT 1 "Sum";
    .port_info 2 /INPUT 1 "A";
    .port_info 3 /INPUT 1 "B";
L_0x558d90b620 .functor NAND 1, v0x558d90b1a0_0, v0x558d90b290_0, C4<1>, C4<1>;
L_0x558d90b730 .functor NAND 1, v0x558d90b1a0_0, L_0x558d90b620, C4<1>, C4<1>;
L_0x558d90b7f0 .functor NAND 1, v0x558d90b290_0, L_0x558d90b620, C4<1>, C4<1>;
L_0x558d90b860 .functor NAND 1, L_0x558d90b620, L_0x558d90b620, C4<1>, C4<1>;
L_0x558d90b950 .functor NAND 1, L_0x558d90b730, L_0x558d90b7f0, C4<1>, C4<1>;
v0x558d90a610_0 .net "A", 0 0, v0x558d90b1a0_0;  1 drivers
v0x558d90a6d0_0 .net "AnandB", 0 0, L_0x558d90b620;  1 drivers
v0x558d90a790_0 .net "B", 0 0, v0x558d90b290_0;  1 drivers
v0x558d90a860_0 .net "Cout", 0 0, L_0x558d90b860;  alias, 1 drivers
v0x558d90a920_0 .net "Sum", 0 0, L_0x558d90b950;  alias, 1 drivers
v0x558d90aa30_0 .net "sum1", 0 0, L_0x558d90b730;  1 drivers
v0x558d90aaf0_0 .net "sum2", 0 0, L_0x558d90b7f0;  1 drivers
S_0x558d90ac30 .scope task, "passTest" "passTest" 2 34, 2 34 0, S_0x558d8ee320;
 .timescale -9 -12;
v0x558d90ae10_0 .var "actualOut", 1 0;
v0x558d90aef0_0 .var "expectedOut", 1 0;
v0x558d90afd0_0 .var "passed", 7 0;
v0x558d90b0c0_0 .var "testType", 120 0;
TD_HalfAddTest_v.passTest ;
    %load/vec4 v0x558d90ae10_0;
    %load/vec4 v0x558d90aef0_0;
    %cmp/e;
    %jmp/0xz  T_1.2, 4;
    %vpi_call 2 39 "$display", "%s passed", v0x558d90b0c0_0 {0 0 0};
    %load/vec4 v0x558d90afd0_0;
    %addi 1, 0, 8;
    %store/vec4 v0x558d90afd0_0, 0, 8;
    %jmp T_1.3;
T_1.2 ;
    %vpi_call 2 40 "$display", "%s failed: %d should be %d", v0x558d90b0c0_0, v0x558d90ae10_0, v0x558d90aef0_0 {0 0 0};
T_1.3 ;
    %end;
    .scope S_0x558d8ee320;
T_2 ;
    %vpi_call 2 30 "$dumpfile", "HalfAddTest.vcd" {0 0 0};
    %vpi_call 2 31 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x558d8ee320 {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x558d8ee320;
T_3 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x558d90b1a0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x558d90b290_0, 0, 1;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x558d90b530_0, 0, 8;
    %delay 90000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x558d90b1a0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x558d90b290_0, 0, 1;
    %delay 10000, 0;
    %load/vec4 v0x558d90b360_0;
    %load/vec4 v0x558d90b460_0;
    %concat/vec4; draw_concat_vec4
    %store/vec4 v0x558d90ae10_0, 0, 2;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x558d90aef0_0, 0, 2;
    %pushi/vec4 3156784, 0, 121;
    %store/vec4 v0x558d90b0c0_0, 0, 121;
    %load/vec4 v0x558d90b530_0;
    %store/vec4 v0x558d90afd0_0, 0, 8;
    %fork TD_HalfAddTest_v.passTest, S_0x558d90ac30;
    %join;
    %load/vec4 v0x558d90afd0_0;
    %store/vec4 v0x558d90b530_0, 0, 8;
    %delay 90000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x558d90b1a0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x558d90b290_0, 0, 1;
    %delay 10000, 0;
    %load/vec4 v0x558d90b360_0;
    %load/vec4 v0x558d90b460_0;
    %concat/vec4; draw_concat_vec4
    %store/vec4 v0x558d90ae10_0, 0, 2;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0x558d90aef0_0, 0, 2;
    %pushi/vec4 3156785, 0, 121;
    %store/vec4 v0x558d90b0c0_0, 0, 121;
    %load/vec4 v0x558d90b530_0;
    %store/vec4 v0x558d90afd0_0, 0, 8;
    %fork TD_HalfAddTest_v.passTest, S_0x558d90ac30;
    %join;
    %load/vec4 v0x558d90afd0_0;
    %store/vec4 v0x558d90b530_0, 0, 8;
    %delay 90000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x558d90b1a0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x558d90b290_0, 0, 1;
    %delay 10000, 0;
    %load/vec4 v0x558d90b360_0;
    %load/vec4 v0x558d90b460_0;
    %concat/vec4; draw_concat_vec4
    %store/vec4 v0x558d90ae10_0, 0, 2;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0x558d90aef0_0, 0, 2;
    %pushi/vec4 3222320, 0, 121;
    %store/vec4 v0x558d90b0c0_0, 0, 121;
    %load/vec4 v0x558d90b530_0;
    %store/vec4 v0x558d90afd0_0, 0, 8;
    %fork TD_HalfAddTest_v.passTest, S_0x558d90ac30;
    %join;
    %load/vec4 v0x558d90afd0_0;
    %store/vec4 v0x558d90b530_0, 0, 8;
    %delay 90000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x558d90b1a0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x558d90b290_0, 0, 1;
    %delay 10000, 0;
    %load/vec4 v0x558d90b360_0;
    %load/vec4 v0x558d90b460_0;
    %concat/vec4; draw_concat_vec4
    %store/vec4 v0x558d90ae10_0, 0, 2;
    %pushi/vec4 2, 0, 2;
    %store/vec4 v0x558d90aef0_0, 0, 2;
    %pushi/vec4 3222321, 0, 121;
    %store/vec4 v0x558d90b0c0_0, 0, 121;
    %load/vec4 v0x558d90b530_0;
    %store/vec4 v0x558d90afd0_0, 0, 8;
    %fork TD_HalfAddTest_v.passTest, S_0x558d90ac30;
    %join;
    %load/vec4 v0x558d90afd0_0;
    %store/vec4 v0x558d90b530_0, 0, 8;
    %delay 90000, 0;
    %load/vec4 v0x558d90b530_0;
    %store/vec4 v0x558d90a330_0, 0, 8;
    %pushi/vec4 4, 0, 8;
    %store/vec4 v0x558d8f1b60_0, 0, 8;
